public class AddTwoNumbers {
    public static void main(String[] args) {
        // Declare two integer variables
        int number1 = 10;
        int number2 = 20;

        // Perform the addition
        int sum = number1 + number2;

        // Print the result
        System.out.println("The sum of " + number1 + " and " + number2 + " is: " + sum);
    }
}
